{
  "name": "amclab/baseline",
  "description": "AmcLab Coding conventions",
  "authors": [{
    "name": "Federico Torrisi",
    "email": "federico@amcweb.it"
  }],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AmcLab\\Baseline\\Providers\\BaselineServiceProvider"
      ],
      "aliases": {
        "Output": "AmcLab\\Baseline\\Facades\\OutputFacade"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "AmcLab\\Baseline\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}